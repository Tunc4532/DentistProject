
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("_PatientLayoutHeadComponentPartial")

<body>
	<div class="wrapper">

		@await Component.InvokeAsync("_PatientLayoutSideBarComponentPartial")

		<div class="main">
			<nav class="navbar navbar-expand navbar-light navbar-bg">
				<a class="sidebar-toggle js-sidebar-toggle">
					<i class="hamburger align-self-center"></i>
				</a>

				<div class="navbar-collapse collapse">
					<ul class="navbar-nav navbar-align">

						@await Component.InvokeAsync("_PatientLayoutNotificationsComponentPartial")

						@await Component.InvokeAsync("_PatientLayoutMessageComponentPartial")

						@await Component.InvokeAsync("_PatientLayoutProfileComponentPartial")

					</ul>
				</div>
			</nav>

			<main class="content">
				<div class="container-fluid p-0">

					<h1 class="h3 mb-3">Blank Page</h1>

					<div class="row">
						<div class="col-12">
							<div class="card">
								<div class="card-header">
									<h5 class="card-title mb-0">Empty card</h5>
								</div>
								<div class="card-body">
									@RenderBody()
								</div>
							</div>
						</div>
					</div>

				</div>
			</main>

			@await Component.InvokeAsync("_PatientLayoutFooterComponentPartial")

		</div>
	</div>

	@await Component.InvokeAsync("_PatientLayoutScriptsComponentPartial")

</body>

</html>